<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="527.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.maua.Controller.Controller">
    <children>
        <VBox layoutX="4.0" layoutY="4.0" prefHeight="398.0" prefWidth="347.0">
            <children>
                <HBox prefHeight="39.0" prefWidth="331.0">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" />
                    </VBox.margin>
                </HBox>
                <Label prefHeight="27.0" prefWidth="90.0" text="Cadastro:">
                    <VBox.margin>
                        <Insets left="16.0" top="16.0" />
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="txtId" promptText="ID:">
                    <VBox.margin>
                        <Insets bottom="8.0" left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="txtNome" promptText="Nome:">
                    <VBox.margin>
                        <Insets bottom="8.0" left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="txtRaridade" promptText="Raridade:">
                    <VBox.margin>
                        <Insets bottom="8.0" left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="txtSerie" promptText="Série:">
                    <VBox.margin>
                        <Insets bottom="8.0" left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="txtColecao" promptText="Coleção:">
                    <VBox.margin>
                        <Insets bottom="8.0" left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="txtUrl" promptText="URL da imagem:">
                    <VBox.margin>
                        <Insets bottom="16.0" left="16.0" right="16.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <ImageView fx:id="imgFoto" fitHeight="129.0" fitWidth="170.0" pickOnBounds="true" preserveRatio="true">
                    <VBox.margin>
                        <Insets bottom="16.0" left="80.0" top="8.0" />
                    </VBox.margin>
                </ImageView>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#cadastrar" prefHeight="25.0" prefWidth="352.0" text="Cadastrar">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            <Button mnemonicParsing="false" onAction="#print" prefHeight="25.0" prefWidth="353.0" text="Print" />
            </children>
        </VBox>
      <VBox layoutX="360.0" layoutY="4.0" prefHeight="518.0" prefWidth="427.0">
         <children>
            <HBox prefHeight="35.0" prefWidth="445.0">
               <children>
                        <Button alignment="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="77.0" text="Previous">
                            <cursor>
                                <Cursor fx:constant="HAND" />
                            </cursor>
                     <padding>
                        <Insets bottom="8.0" top="8.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="8.0" left="8.0" top="8.0" />
                     </HBox.margin></Button>
                        <Button alignment="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" text="Next">
                            <cursor>
                                <Cursor fx:constant="HAND" />
                            </cursor>
                     <HBox.margin>
                        <Insets bottom="8.0" left="256.0" top="8.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="8.0" top="8.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
         </children></VBox>
    </children>
</AnchorPane>
